{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Start MCP",
            "request": "launch",
            "mainClass": "Start",
            "projectName": "MCP",
            "javaExec": "/usr/lib/jvm/java-21-openjdk/bin/java",
            "cwd": "${env:HOME}/.minecraft/",
            "vmArgs": "-Djava.library.path=${env:HOME}/.minecraft/test_natives",
        }
    ]
}